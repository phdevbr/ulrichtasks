<%= form_with url: login_path do |f| %>
  <div>
    <%= f.label :username, style: "display:block" %>
    <%= f.text_field :username %>
  </div>

  <div>
    <%= f.label :password, style: "display: block" %>
    <%= f.text_field :password %>
  </div>

  <div>
    <%= f.submit "Login" %>
  </div>
<% end %>
